{
  "name": "cross-domain-recommendation-engine",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next out *.tsbuildinfo && npm run clear:cache",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:integration": "vitest run src/__tests__/integration --reporter=verbose",
    "test:integration:e2e": "vitest run src/__tests__/integration/e2e-integration.test.ts --reporter=verbose",
    "test:integration:performance": "vitest run src/__tests__/integration/performance.test.ts --reporter=verbose",
    "test:integration:load": "vitest run src/__tests__/integration/load-test.test.ts --reporter=verbose",
    "test:integration:real": "INTEGRATION_TEST=true vitest run src/__tests__/integration/real-api-integration.test.ts --reporter=verbose",
    "test:integration:runner": "node scripts/run-integration-tests.js",
    "test:api": "node test-api-keys.js",
    "clear:cache": "node clear-cache.js",
    "migrate": "node scripts/migrate-database.js",
    "migrate:prod": "NODE_ENV=production node scripts/migrate-database.js",
    "db:backup": "node scripts/migrate-database.js --backup-only",
    "db:types": "supabase gen types typescript --local > src/types/supabase.ts",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.27.1",
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@supabase/supabase-js": "^2.52.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.532.0",
    "next": "15.4.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "redis": "^5.6.1",
    "svix": "^1.69.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/redis": "^4.0.10",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "jsdom": "^26.1.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
